{
  "name": "redux-dataloader",
  "version": "0.0.6",
  "description": "Loads async data for Redux apps focusing on preventing duplicated requests and dealing with async dependencies.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kouhin/redux-dataloader.git"
  },
  "bugs": {
    "url": "https://github.com/kouhin/redux-dataloader/issues"
  },
  "homepage": "https://github.com/kouhin/redux-dataloader",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "build": "npm run clean && npm run build:commonjs && npm run build:umd",
    "build:commonjs": "babel src --out-dir lib",
    "build:umd": "webpack",
    "prepublish": "npm run clean && npm run lint && npm test && npm run build",
    "test": "babel-node $(npm bin)/isparta cover $(npm bin)/_mocha test/*.js -- --opts mocha.opts",
    "lint": "eslint src test"
  },
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "keywords": [
    "redux",
    "react",
    "middleware",
    "redux-middleware",
    "data",
    "loading",
    "async",
    "effects",
    "side effects",
    "isomorphic"
  ],
  "author": "Bin Hou",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.2.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-import": "^1.4.0",
    "eslint-plugin-react": "^4.3.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "lodash": "^4.4.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint-config-airbnb",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "plugins": [
      "react",
      "import"
    ]
  },
  "babel": {
	  "presets": [
		  "es2015",
		  "react",
		  "stage-0"
	  ],
    "plugins": [
      "transform-runtime"
    ]
  }
}
